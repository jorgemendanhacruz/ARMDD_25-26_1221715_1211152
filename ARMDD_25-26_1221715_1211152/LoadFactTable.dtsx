    CAST(old.PromotionDiscount AS decimal(12,4)) AS PromotionDiscountLocal,
    CAST(ol.LineTotal AS decimal(12,4)) AS TotalAmountLocal
FROM dbo.OrdersLines ol
JOIN dbo.Orders o
  ON o.OrderID = ol.OrderID
LEFT JOIN dbo.OrdersLinesDetails old
  ON old.OrderLineID = ol.OrderLineID;</property>
